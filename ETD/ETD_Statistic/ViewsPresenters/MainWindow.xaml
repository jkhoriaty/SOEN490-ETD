<Window x:Class="ETD_Statistic.ViewsPresenters.MainWindow"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:properties="clr-namespace:ETD_Statistic.Properties"
        Title="Emergency Team Dispatcher Statistic"
		WindowStartupLocation="CenterScreen"
		WindowState="Maximized"
		MinWidth="1200"
		Width="1200"
		MinHeight="700"
		Height="700">
    <Grid>
        <Grid Margin="0,0,0,650">
            <Button x:Uid="Button_LoadStatistic" Click="LoadStatistic" Content="{x:Static properties:Resources.Button_LoadStatistic}" Height="30" Width="100"/>
        </Grid>
        <Grid Margin="0,100,0,0">
            
        </Grid>
    </Grid>
</Window>
