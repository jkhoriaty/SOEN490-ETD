<Page x:Class="ETD.ViewsPresenters.MapSection.MapSectionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="MapSectionPage">

    <Canvas Name="Map">


    </Canvas>
    <Page.Resources>
		<ContextMenu x:Key="TeamContext" Opened="EditMenuItems">
			<MenuItem Header="Available" Tag="team" Click="ChangeStatus"/>
			<MenuItem Header="Moving" Tag="team" Click="ChangeStatus"/>
			<MenuItem Header="Intervening" Tag="team" Click="ChangeStatus"/>
			<MenuItem Header="Unavailable" Tag="team" Click="ChangeStatus"/>
			<MenuItem Header="Delete Equipment" Tag="equipment" Click="DeleteEquipment"/>
		</ContextMenu>
    </Page.Resources>
</Page>
