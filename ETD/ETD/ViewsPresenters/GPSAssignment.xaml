<Window x:Class="ETD.ViewsPresenters.GPSAssignment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GPSAssignment" Height="300" Width="300">
    <StackPanel>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" SharedSizeGroup="Label"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>
            <Label Grid.Row="0" Grid.Column="0" Content="Team Name: "></Label>
            <ComboBox Grid.Row="0" Grid.Column="1" Name="teamCB"></ComboBox>
            <Label Grid.Row="1" Grid.Column="0" Content="Member Name: "></Label>
            <ComboBox Grid.Row="1" Grid.Column="1" Name="memberCB"></ComboBox>
        </Grid>

        <Button x:Uid="Button_Submit" Click="Submit_Click" Width="100" Content="{Binding Path=Button_Submit, Source={StaticResource Resources}}" />
    </StackPanel>
</Window>
